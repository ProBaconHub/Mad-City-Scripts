        local BreakInTelePortSection = BreakInTab:NewSection("Teleports")
        _G.BreakInRoleName = "Bat"
        _G.BreakInRoleKid = false
        BreakInLobbySection:NewDropdown("Roles", "Become role (Only in lobby)", {"Bat", "MedKit", "TeddyBloxpin", "Chips"}, function(currentOption)
            _G.BreakInRoleName = currentOption
