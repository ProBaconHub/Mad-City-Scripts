                [1] = _G.BreakInRoleName,
                [2] = _G.BreakInRoleKid,
                [3] = true
            }
            game:GetService("ReplicatedStorage").RemoteEvents.MakeRole:FireServer(unpack(args))
        end)
        BreakInLobbySection:NewDropdown("OutSideRole", "Become role (Only in lobby)", {"SwatGun", "Lollipop", "Gun"}, function(currentOption)
            _G.BreakInRoleName = currentOption
            local args = {
