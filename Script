--New Tab Function
function AddTab(Tab)
    if Tab == "Adopt Me!" then
        --Adopt
        local AdoptMeTab = Window:NewTab("Adopt Me!")
        local AdoptMeFarmSection = AdoptMeTab:NewSection("Auto Farm")
        local PetSection = AdoptMeTab:NewSection("Pet")

        AdoptMeFarmSection:NewButton("Dev finding for Baby Auto Farm", "Voluntear to help Pro Bacon", function()
            clipboardCopy("Discord invite link")
        end)

        PetSection:NewButton("Dev finding for Neon Pet", "Voluntear to help Pro Bacon", function()
            clipboardCopy("Discord invite link")
        end)
        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Done!"; Duration=5;})


    elseif Tab == "Build a Boat" then
        --Build
        local BuildaBoatTab = Window:NewTab("Build a Boat")
        local BABFTFarmSection = BuildaBoatTab:NewSection("Auto Farm")
        local BABFTTPSection = BuildaBoatTab:NewSection("Teletort")
        
        BABFTFarmSection:NewButton("Dev finding for Gold Auto farm", "Voluntear to help Pro Bacon", function()
            clipboardCopy("Discord invite link")
        end)
        BABFTFarmSection:NewLabel("Gold block autofarm WIP")

        BABFTTPSection:NewLabel("Dropdown comming soon!")

        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Done!"; Duration=5;})

    elseif Tab == "Mad City CH2" then
        --Mad
        local MadCityTab = Window:NewTab("Mad City")
        local MadAutoSection = MadCityTab:NewSection("Auto")
        local MadRemoteSection = MadCityTab:NewSection("Remote")
        local MadTeleSection = MadCityTab:NewSection("Force Teleport")

        MadAutoSection:NewDropdown("Auto Escape", "Choose what type of auto escape", {"Escape", "Escape + Back to base", "Escape + Go to pear shop"}, function(currentOption)
            if currentOption == "Escape" then
                loadstring(Escape)()
            elseif currentOption == "Escape + Back to base" then
                loadstring(EscapeToBace)()
            elseif currentOption == "Escape + Go to pear shop" then
                loadstring(EscapeToPear)()
            end
        end)
        MadAutoSection:NewButton("Aim", "Aim good", function()
            loadstring(Aim)()
        end)
        MadAutoSection:NewSlider("Aim Circle", "Decide how big the circle is.", 1500, 0, function(AimCircle)
            getgenv().fov = AimCircle
        end)
        MadAutoSection:NewButton("Half-Auto Rob (Patched)", "Auto Rob but you have to spam E by yourself, TP patched", function()
            loadstring(HalfAutoRob)()
        end)

        MadRemoteSection:NewButton("No E Wait", "No more waiting when pressing E!", function()
            loadstring(NoEWait)()
        end)
        MadRemoteSection:NewButton("Car Mode", "Make your car way more better!", function()
            loadstring(CarMode)()
        end)
        MadRemoteSection:NewToggle("Motor", "Enable if you are not using a car (Includ motor like Hyper Glider)", function(state)
            if state then
                _G.Hyper = true --I dont know if only hyper will glitch out because I dont have other moters
            else
                _G.Hyper = false
            end
        end)
        MadTeleSection:NewDropdown("Force Teleport", "It will TP you back after", {"Base", "Casino", "Bank", "Club", "Prymid"}, function(currentOption)
            if currentOption == "Base" then
                for i = 1,200 do
	                task.wait()
	                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(106, 26, -166)
                end
            elseif currentOption == "Casino" then
                for i = 1,200 do
	                task.wait()
	                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1500, -41, 622)
                end
            elseif currentOption == "Bank" then
                for i = 1,200 do
	                task.wait()
	                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(653, -19, 566)
                end
            elseif currentOption == "Club" then
                for i = 1,200 do
	                task.wait()
	                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1835, 225, -1167)
                end
            elseif currentOption == "Prymid" then
                for i = 1,200 do
	                task.wait()
	                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(162, -62, 1152)
                end
            end
        end)

        game.StarterGui:SetCore("SendNotification", {
            Icon = " ",
             Title = "Pro Bacon",
             Text = "Done"
        })


    elseif Tab == "Punch Wall Simulator" then
        --Pws
        local PunchWallSimTab = Window:NewTab("PunchWallSim")
        local PwsCashSection = PunchWallSimTab:NewSection("Cash")
        local PwsPunchSection = PunchWallSimTab:NewSection("Punch")
        local PwsGiftSection = PunchWallSimTab:NewSection("Gift")
        local PwsSpinSection = PunchWallSimTab:NewSection("Spin")
        local PwsTipSection = PunchWallSimTab:NewSection("Glitch Tip")
        local PwsFarmSection = PunchWallSimTab:NewSection("Farm")
        _G.PwsFarmMode = "Normal"
        _G.PwsFarmEnable = false

        PwsCashSection:NewButton("Gimmi Cash!", "Give you money by compleating lvl 10", function()
            game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
        end)
        PwsPunchSection:NewButton("Gimmi Punch!", "Give you more punch!", function()
            game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        end)
        PwsGiftSection:NewDropdown("Get Gift", "Choose what gift u wanna get", {1, 2, 3, 4, 5, 6, 7, 8}, function(currentOption)
            game:GetService("ReplicatedStorage").Event.OnlineGift:FireServer(currentOption)
        end)
        PwsGiftSection:NewButton("Clam All Gift! (OP)", "Clam all gift without getting them, can do many times By: Pro Bacon", function()
            for i = 1, 8 do
                game:GetService("ReplicatedStorage").Event.OnlineGift:FireServer(i)
                wait()
            end
        end)
        PwsSpinSection:NewDropdown("Get Spin", "Choose what prise u wanna get", {1, 2, 3, 4, 5}, function(currentOption)
            game:GetService("ReplicatedStorage").Event.Spin:FireServer(currentOption)
        end)
        PwsSpinSection:NewButton("Get All Spin! (OP)", "Get all spin By: Pro Bacon", function()
            for i = 1, 5 do
                game:GetService("ReplicatedStorage").Event.Spin:FireServer(i)
                wait()
            end
        end)
        PwsTipSection:NewButton("Get Tip", "Wot tip?", function()
            game.StarterGui:SetCore("SendNotification", {
                Icon = " ",
                 Title = "Pro Bacon",
                 Text = "Run while jump can step on your pet"
            })
            game.StarterGui:SetCore("SendNotification", {
                Icon = " ",
                 Title = "Pro Bacon",
                 Text = "It will make you jump higher and faster"
            })
        end)
        PwsFarmSection:NewButton("Gimmi All! (OP)", "By: Pro Bacon", function()
            loadstring(Game:HttpGet("https://raw.githubusercontent.com/ProBaconHub/ProBaconGUI/main/PwsFarmProBaconBestMode", true))()
        end)
        PwsFarmSection:NewToggle("Auto Farm", "Auto Farm", function(state)
            if state then
                _G.PwsFarmEnable = true
                loadstring(PwsFarm)()
            else
                _G.PwsFarmEnable = false
            end
        end)
        PwsFarmSection:NewDropdown("Auto Farm Mode", "Choose Farm Mode", {"Normal", "Fast", "Super", "Ultra", "Safe", "ProBaconBest"}, function(currentOption)
            _G.PwsFarmMode = currentOption
        end)
        game.StarterGui:SetCore("SendNotification", {
            Icon = " ",
             Title = "Pro Bacon",
             Text = "Done"
        })
    elseif Tab == "TLOTBS RPG" then
        --The Legend of The Bone Sword RPG
        local TLOTBSTab = Window:NewTab("TLOTBST")
        local LotbsAutoSection = TLOTBSTab:NewSection("The Legend Of The Bone Sword")
        local LotbsTPSection = TLOTBSTab:NewSection("TP")
        local LotbsBossSection = TLOTBSTab:NewSection("Boss")

        LotbsAutoSection:NewToggle("Auto Click", "Auto hit surrouning mobs when equip sword", function(state)
            _G.LotbsAutoHit = state
            loadstring(TLOTBSautoclick)()
        end)
        LotbsTPSection:NewTextBox("PlayerTP", "Input player name", function(txt)
            local GamePlayer = game.Players
            local Plr = GamePlayer.txt
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Plr.Character.HumanoidRootPart.CFrame
        end)
        LotbsBossSection:NewTextBox("Auto Hit Boss", "Input boss name", function(txt)
            _G.EnemiesName = txt
        end)
        LotbsBossSection:NewButton("Print All Enemies", "It will print in Dev Console", function()
            _G.LastEnemie = Nothing
            _G.MixResult = nil
            print("--------All Enemies--------")
            for i,v in pairs(Workspace.Enemies:GetChildren()) do
            	if v.Name ~= _G.LastEnemie then
            		_G.LastEnemie = v.Name
            		print(v.Name)
            	end
            end
            print("Done")
        end)
        LotbsBossSection:NewToggle("On/Off Auto Hit", "Turn on and off", function(state)
            _G.AutoHitEnable = state
            loadstring(TLOTBSautohit)()
        end)

        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Done!"; Duration=5;})

    elseif Tab == "Break In (Story)" then
        local BreakInTab = Window:NewTab("BreakInStory")
        local BreakInLobbySection = BreakInTab:NewSection("Lobby")
        local BreakInGameSection = BreakInTab:NewSection("In Game")
        local BreakInCatSection = BreakInTab:NewSection("Cat")
