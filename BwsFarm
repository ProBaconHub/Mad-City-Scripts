while _G.BwsFarmEnable == true do
    if _G.BwsFarmMode == Normal then --How normal
        wait()
        game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
    elseif _G.BwsFarmMode == Fast then --Lets be faster
        wait()
        game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
        game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
    elseif _G.BwsFarmMode == Super then
        game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
        game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
        for i = 1, 8 do
            game:GetService("ReplicatedStorage").Event.OnlineGift:FireServer(i)
            wait()
        end
        for i = 1, 5 do
            game:GetService("ReplicatedStorage").Event.Spin:FireServer(i)
            wait()
        end
    elseif _G.BwsFarmMode == Ultr" then    --SUPER ULTRA!!! But...May Lag T_T
        game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
        game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
        game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
        game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
        game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
        game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
        game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
        game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
        game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
        game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
        game:GetService("ReplicatedStorage").Event.OnlineGift:FireServer(1)
        game:GetService("ReplicatedStorage").Event.OnlineGift:FireServer(2)
        game:GetService("ReplicatedStorage").Event.OnlineGift:FireServer(3)
        game:GetService("ReplicatedStorage").Event.OnlineGift:FireServer(4)
        game:GetService("ReplicatedStorage").Event.OnlineGift:FireServer(5)
        game:GetService("ReplicatedStorage").Event.OnlineGift:FireServer(6)
        game:GetService("ReplicatedStorage").Event.OnlineGift:FireServer(7)
        game:GetService("ReplicatedStorage").Event.OnlineGift:FireServer(8)
        game:GetService("ReplicatedStorage").Event.Spin:FireServer(1)
        game:GetService("ReplicatedStorage").Event.Spin:FireServer(2)
        game:GetService("ReplicatedStorage").Event.Spin:FireServer(3)
        game:GetService("ReplicatedStorage").Event.Spin:FireServer(4)
        game:GetService("ReplicatedStorage").Event.Spin:FireServer(5)
    elseif _G.BwsFarmMode == Safe then --How peaseful
        wait(1)
        game:GetService("ReplicatedStorage").Event.TrainHit:FireServer(game:GetService("Players").LocalPlayer)
        game:GetService("ReplicatedStorage"):WaitForChild("Event"):WaitForChild("DestoryWall"):FireServer(10)
        for i = 1, 8 do
            game:GetService("ReplicatedStorage").Event.OnlineGift:FireServer(i)
            wait()
        end
        for i = 1, 5 do
            game:GetService("ReplicatedStorage").Event.Spin:FireServer(i)
            wait()
        end
    end
end
