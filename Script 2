            loadstring(game:HttpGet("https://pastebin.com/raw/7pnyZ2YV", true))()
        end)
    
        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Done!"; Duration=5;})
    elseif Tab == "arm wrestling simulator" then
        local AWSTab = Window:NewTab("arm wrestling simulator")
        local AWS = AWSTab:NewSection("train all")
        
        AWS:NewButton("Train All (OP)", "A train all script that makes you be top in no time!", function()
            workspace.Main.RemoteEvent:FireServer(500000000000000000) workspace.Main2.RemoteEvent:FireServer(500000000000000000) workspace.Main3.RemoteEvent:FireServer(500000000000000000) game:GetService("ReplicatedStorage").money:FireServer(1000000000)
        end)
        AWS:NewButton("[Dangerous] INF Train (Read information!!!)", "This makes you get infinity everything but there is a risk, Click Print Risk!", function()
            workspace.Main.RemoteEvent:FireServer(math.huge) workspace.Main2.RemoteEvent:FireServer(math.huge) workspace.Main3.RemoteEvent:FireServer(math.huge) game:GetService("ReplicatedStorage").money:FireServer(math.huge)
        end)
        AWS:NewButton("Print Risk", "Print the risk", function()
            game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="This script might bring you to a risk that all your state become [nan] and there is no way back!"; Duration=10;})
        end)

        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Done!"; Duration=5;})

    elseif Tab == "Wall Mining Sim" then
        local WallMiningSimTab = Window:NewTab("Wall Mining Sim")
        local WallMiningSimFarm = WallMiningSimTab:NewSection("Farm")
        
        WallMiningSimFarm:NewButton("Add Win", "Get 10QI wins!", function()
            game:GetService("ReplicatedStorage").Remote.Event.Eco["[C-S]AddWin"]:FireServer(9999999999000000000,"EntityDied")
        end)
        WallMiningSimFarm:NewToggle("Add train", "Train for ya!", function(state)
            _G.WallMiningSimFarmTrain = state
            while _G.WallMiningSimFarmTrain and wait() do
                game:GetService("ReplicatedStorage").Remote.Event.Game["[C-S]PlayerPick"]:FireServer("BedRock")
            end
        end)
        WallMiningSimFarm:NewButton("EGG", "Get Egg", function()
            for i = 1,3 do
                game:GetService("ReplicatedStorage").Remote.Function.Luck["[C-S]DoLuck"]:InvokeServer("Robux12")
                game:GetService("ReplicatedStorage").Remote.Function.Luck["[C-S]DoLuck"]:InvokeServer("Robux12")
                game:GetService("ReplicatedStorage").Remote.Function.Luck["[C-S]DoLuck"]:InvokeServer("Robux12")
                game:GetService("ReplicatedStorage").Remote.Event.Pet["[C-S]TryCraftAll"]:FireServer()
            end
            task.wait(.1)
            for i = 1,5 do
                game:GetService("ReplicatedStorage").Remote.Event.Pet["[C-S]TryCraftAll"]:FireServer()
                game:GetService("ReplicatedStorage").Remote.Event.Pet["[C-S]TryEquipBest"]:FireServer()
            end
            task.wait(.5)
            game:GetService("ReplicatedStorage").Remote.Event.Pet["[C-S]TryEquipBest"]:FireServer()
        end)

        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Done!"; Duration=5;})
    elseif Tab == "Sonic Speed Sim" then
        local SonicSpeedSimTab = Window:NewTab("Sonic Speed Sim")
        local SonicSpeedFarm = SonicSpeedSimTab:NewSection("Auto Farm")
        local SonicSpeedBoss = SonicSpeedSimTab:NewSection("Boss Farm")
        local Exp = game:GetService("ReplicatedStorage").Knit.Services.TrialService.RE.OnStatUpdate
        local Rebirth = game:GetService("ReplicatedStorage").Knit.Services.SSLevelingService.RF.AttemptRebirth
        local Prop = {
            [1] = "Emerald Hill",
            [2] = "3rd",
            [3] = "BestRank",
            [4] = 2,
            [5] = false
        }
        SonicSpeedFarm:NewToggle("Auto Farm", "Farm everything!", function(state)
            _G.SonicAutoFarm = state
            while _G.SonicAutoFarm do
                for i = 1,100 do
                    for i = 1,5 do
                        task.wait(0.00001)
                        Exp:FireServer(unpack(Prop))
                        Exp:FireServer(unpack(Prop))
                        Exp:FireServer(unpack(Prop))
                        Exp:FireServer(unpack(Prop))
                        Exp:FireServer(unpack(Prop))
                    end
                    if not _G.SonicAutoFarm then
                        break
                    end
                    Race:FireServer(unpack(RaceProp))
                    Rebirth:InvokeServer(true)
                    task.wait(1)
                end
                if not _G.SonicAutoFarm then
                    break
                end
                game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Waiting to reduce lag!"; Duration=10;})
                wait(10)
                game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="We are back!"; Duration=10;})
            end
        end)

        SonicSpeedFarm:NewToggle("Farm Orb", "Auto collect orbs", function(state)
            _G.SonicAutoOrb = state
            while _G.SonicAutoOrb do
                task.wait(0.00001)
                Exp:FireServer(unpack(Prop))
                Exp:FireServer(unpack(Prop))
                Exp:FireServer(unpack(Prop))
                Exp:FireServer(unpack(Prop))
                Exp:FireServer(unpack(Prop))
            end
        end)

        SonicSpeedFarm:NewToggle("Auto Rebirth", "Auto rebirth", function(state)
            _G.SonicAutoRebirth = state
            while _G.SonicAutoRebirth do
                task.wait(5)
                Rebirth:InvokeServer(true)
            end
        end)


        SonicSpeedBoss:NewButton("Toggle Auto Farm Boss", "Auto join and damage boss!", function()
            _G.SonicAutoBoss = not _G.SonicAutoBoss
            while _G.SonicAutoBoss and task.wait(0.001) do
                game:GetService("ReplicatedStorage").Knit.Services.BossBattleService.RF.JoinQueue:InvokeServer("Emerald Hill Boss Battle")
            	for i = 1,12 do
            		game:GetService("ReplicatedStorage").Knit.Services.BossBattleService.RE.UpdateBattleState:FireServer("GeneratorDamaged",{["Generator"]=i})
            		task.wait(.01)
            	end
            	for i = 1,3 do
            		game:GetService("ReplicatedStorage").Knit.Services.BossBattleService.RE.UpdateBattleState:FireServer("Damage",{})
            		task.wait(0.01)
            	end
            end
            game:GetService("ReplicatedStorage").Knit.Services.BossBattleService.RF.LeaveQueue:InvokeServer("Emerald Hill Boss Battle")
        end)

        SonicSpeedBoss:NewButton("Damage Generator", "Instanse damage all genarator", function()
            for i = 1,12 do
                game:GetService("ReplicatedStorage").Knit.Services.BossBattleService.RE.UpdateBattleState:FireServer("GeneratorDamaged",{["Generator"]=i})
            end
        end)

        SonicSpeedBoss:NewButton("Damage Boss", "Must damage all genarator first", function()
            for i = 1,3 do
                game:GetService("ReplicatedStorage").Knit.Services.BossBattleService.RE.UpdateBattleState:FireServer("Damage",{})
                task.wait(0.01)
            end
        end)

        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Done!"; Duration=5;})
    elseif Tab == "My Prison" then
        local MyPrisonTab = Window:NewTab("MyPrison")
        local MyPrison = MyPrisonTab:NewSection("MyPrison")
        local MyPrisonEvent = MyPrisonTab:NewSection("MyPrisonEvent")
        local Player = game.Players.LocalPlayer.Character.HumanoidRootPart
        local vim = game:GetService('VirtualInputManager')
        function Click(key, HT, cd)
            vim:SendKeyEvent(true, key, false, game)
            task.wait(HT)
            vim:SendKeyEvent(false, key, false, game)
            task.wait(cd)
        end

        MyPrison:NewButton("Arrest", "Go to a place first.", function()
            for i = 1, 500 do
                game:GetService("ReplicatedStorage").Remote.NPC.Interact:FireServer(i,"PromptTriggered")
            end
        end)
        MyPrisonEvent:NewButton("Collect All Baterys", "Collect baterys instancely", function()
            game:GetService("ReplicatedStorage").Remote.Client:FireServer("CollectBattery","Battery_1")
            game:GetService("ReplicatedStorage").Remote.Client:FireServer("CollectBattery","Battery_2")
            game:GetService("ReplicatedStorage").Remote.Client:FireServer("CollectBattery","Battery_3")
            game:GetService("ReplicatedStorage").Remote.Client:FireServer("CollectBattery","Battery_4")
            game:GetService("ReplicatedStorage").Remote.Client:FireServer("CollectBattery","Battery_5")
            game:GetService("ReplicatedStorage").Remote.Client:FireServer("CollectBattery","Battery_6")
            game:GetService("ReplicatedStorage").Remote.Client:FireServer("CollectBattery","Battery_7")
        end)
        MyPrisonEvent:NewButton("Auto Place Baterys", "Auto place batterys.", function()
            Player.CFrame = game.Workspace.Batterys.Battery_1.Part.CFrame
            task.wait(0.1)
            Click(Enum.KeyCode.E, 0.1, .01)
            task.wait(0.1)

            Player.CFrame = game.Workspace.Batterys.Battery_2.Part.CFrame
            task.wait(0.1)
            Click(Enum.KeyCode.E, 0.1, .01)
            task.wait(0.1)

            Player.CFrame = game.Workspace.Batterys.Battery_3.Part.CFrame
            task.wait(0.1)
            Click(Enum.KeyCode.E, 0.1, .01)
            task.wait(0.1)

            Player.CFrame = game.Workspace.Batterys.Battery_4.Part.CFrame
            task.wait(0.1)
            Click(Enum.KeyCode.E, 0.1, .01)
            task.wait(1)

            Player.CFrame = game.Workspace.Batterys.Battery_5.Part.CFrame
            task.wait(0.1)
            Click(Enum.KeyCode.E, 0.1, .01)
            task.wait(0.1)

            Player.CFrame = game.Workspace.Batterys.Battery_6.Part.CFrame
            task.wait(0.1)
            Click(Enum.KeyCode.E, 0.1, .01)
            task.wait(1)

            Player.CFrame = game.Workspace.Batterys.Battery_7.Part.CFrame
            task.wait(0.1)
            Click(Enum.KeyCode.E, 0.1, .01)
        end)
        MyPrisonEvent:NewButton("Auto put out fire", "Fill and pour water.", function()
            for i = 1, 10 do
                Player.CFrame = game:GetService("Workspace").WaterZone.CFrame
                task.wait(0.1)
                Player.CFrame = game:GetService("Workspace").Plots.Plot2.Floors["2"].Furnitures.Fireplace.Models.Model:GetChildren()[3].CFrame
                task.wait(0.1)
            end
        end)
        MyPrisonEvent:NewLabel("For event: You dont have to go anywhere")
        MyPrisonEvent:NewLabel("Game will teleport you.")
    end
end


