SettingSection:NewKeybind("Toggle GUI", "The input key will be the key to toggle GUI", Enum.KeyCode.Delete, function()
	Library:ToggleUI()
end)
SettingSection:NewButton("Update Function", "Update it when there is any changes", function()
    UpdateFunctions()
end)
SettingSection:NewButton("Check for function update", "Check for new update for function!", function()
    if VersionOfFunction == game:HttpGet("https://raw.githubusercontent.com/ProBaconHub/ProBaconGUI/main/-Version%20(Functuion)", {["Cache-Control"] = "no-cache"}) then
        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Function is up to date!"; Duration=5;})
    else
        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="There is new update for function, requier Update function to get the newest version of function!"; Duration=10;})
    end
end)
SettingSection:NewButton("Check for GUI update", "Check for new update for function!", function()
    print(SelfVersion)
    print(VersionOfGUI)
    if VersionOfGUI == SelfVersion then
        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="No update for GUI!"; Duration=5;})
    else
        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="There is new version of GUI is out! There might be more supported games of fixes! Requier rexecute to update!"; Duration=10;})
    end
end)
SettingSection:NewButton("Check for GUI version", "See GUI version", function()
    print("--------GUI state--------")
    print("Current version of GUI: ",SelfVersion)
    print("Current version of function: ",VersionOfFunction)
    print("Available version of GUI: ", VersionOfGUI)
    print("Available version of function: ",VersionOfFunction)
    print("Update function to get 100% correct info")
    game.StarterGui:SetCore("DevConsoleVisible", true)
end)





























--New Tab Function
function AddTab(Tab)
    if Tab == "Adopt Me!" then
        --Adopt
        local AdoptMeTab = Window:NewTab("Adopt Me!")
        local AdoptMeFarmSection = AdoptMeTab:NewSection("Auto Farm")
        local PetSection = AdoptMeTab:NewSection("Pet")

        AdoptMeFarmSection:NewButton("Dev finding for Baby Auto Farm", "Voluntear to help Pro Bacon", function()
            clipboardCopy("Discord invite link")
        end)

        PetSection:NewButton("Dev finding for Neon Pet", "Voluntear to help Pro Bacon", function()
            clipboardCopy("Discord invite link")
        end)
        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Done!"; Duration=5;})


    elseif Tab == "Build a Boat" then
        --Build
        local BuildaBoatTab = Window:NewTab("Build a Boat")
        local BABFTFarmSection = BuildaBoatTab:NewSection("Auto Farm")
        local BABFTTPSection = BuildaBoatTab:NewSection("Teletort")
        
        BABFTFarmSection:NewButton("Dev finding for Gold Auto farm", "Voluntear to help Pro Bacon", function()
            clipboardCopy("Discord invite link")
        end)
        BABFTFarmSection:NewLabel("Gold block autofarm WIP")

        BABFTTPSection:NewLabel("Dropdown comming soon!")

        game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Done!"; Duration=5;})

    elseif Tab == "Mad City CH2" then
        --Mad
        local MadCityTab = Window:NewTab("Mad City")
        local MadAutoSection = MadCityTab:NewSection("Auto")
        local MadRemoteSection = MadCityTab:NewSection("Remote")
        local MadTeleSection = MadCityTab:NewSection("Teleport")
        local MadLazerControlSection = MadCityTab:NewSection("Lazer Control")
        local MadHeistControlSection = MadCityTab:NewSection("Heist Control")
        local MadEventSection = MadCityTab:NewSection("Piggy Event")
        local u1 = require(game:GetService("ReplicatedStorage").Aero.Shared.Util)
        function Teleport(x,y,z,cd)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(x,y,z)
            task.wait(cd)
        end
        
        function TweenTeleport(x,y,z, time)
            game.Players.LocalPlayer.Character.Humanoid.Sit = false
            local Position = Vector3.new(x,y,z)
            local TweenService = game:GetService('TweenService')
            local LocalPlayer = game.Players.LocalPlayer
            local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:wait()
            local Tween = TweenService:create(Character.Humanoid.RootPart,TweenInfo.new(time --[[ time ]]), {CFrame = CFrame.new(Position)}  )
            Tween:Play()
            game.Players.LocalPlayer.Character.Humanoid.Sit = false
            task.wait(time)
        end
        
        function EscapeCell()
            u1.Network:Invoke("RequestTeamChange", "Prisoners")
            task.wait(1)
            game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Escaping cell..."; Duration=5;})
            TweenTeleport(-2092, 88, -538, 0.25)
            TweenTeleport(-2097, 88, -587, 0.25)
            TweenTeleport(-2093, 88, -561, 0.25)
            TweenTeleport(-2092, 88, -538, 0.25)
            TweenTeleport(-2092, 73, -538, 0.25)
            TweenTeleport(-2097, 73, -587, 0.25)
            TweenTeleport(-2093, 73, -561, 0.25)
            TweenTeleport(-2092, 73, -538, 0.25)
            TweenTeleport(-2092, 61, -538, 0.25)
            TweenTeleport(-2097, 61, -587, 0.25)
            TweenTeleport(-2093, 61, -561, 0.25)
            TweenTeleport(-2092, 56, -538, 0.25)
            TweenTeleport(-2092, 61, -538, 0.25)
            TweenTeleport(-2097, 56, -587, 0.25)
            TweenTeleport(-2093, 56, -561, 0.25)
            TweenTeleport(-2044, 47, -560, 0.25)
            TweenTeleport(-2092, 56, -538, 0.25)
            TweenTeleport(-2060, 47, -540, 0.25)
            TweenTeleport(-2044, 47, -560, 0.25)
            TweenTeleport(-2028, 47, -551, 0.25)
            TweenTeleport(-2047, 47, -539, 0.25)
            TweenTeleport(-2126, 47, -529, 1)
        end

        MadAutoSection:NewDropdown("Auto Escape", "Choose what type of auto escape", {"Escape", "Escape + Back to base", "Escape + Go to pear shop"}, function(currentOption)
            if currentOption == "Escape" then
                loadstring(Escape)()
            elseif currentOption == "Escape + Back to base" then
                loadstring(EscapeToBace)()
