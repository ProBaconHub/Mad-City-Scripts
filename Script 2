            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1663, 210, 614)
            task.wait(2)
            for i = 1,20 do
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1663, 210, 614)
            end
            for i, deleteparts in next, game:GetService("Workspace")[game.Players.LocalPlayer.Name]:GetChildren() do
                if deleteparts.Name == "HumanoidRootPart" or deleteparts.Name == "BillboardGui" or deleteparts.Name == "Humanoid" or deleteparts.Name == "Animate" or deleteparts.Name == "Health" or deleteparts.Name ==  "Parachte" or deleteparts:IsA("Accessory") or deleteparts:IsA("BodyColors") or deleteparts:IsA("MeshPart") or deleteparts:IsA("Pants") or deleteparts:IsA("Shirt") --[[]] then
                else
                    deleteparts:Destory()
                end
            end
            for i = 1,30 do
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Heists.Casino.Items.TouchGrapple.CFrame
                task.wait(0.01)
                if i == 10 then
                    v1.Network:Fire("RagdollUpdate", true)
                end
            end
            task.wait(1)
            v1.Network:Fire("RagdollUpdate", false)
        end)
