coroutine.wrap(OPWZ_fake_script)()
local function COOMEEN_fake_script() -- Launch.LocalScript 
	local script = Instance.new('LocalScript', Launch)

	local function LeftClickOnLaunch()
		if _G.ProBaconHubLoadDataDone then
			game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="May take a few seconds to load..."; Duration=5;})
			loadstring(game:HttpGet("https://raw.githubusercontent.com/ProBaconHub/ProBaconGUI/main/Script", {["Cache-Control"] = "no-cache"}))()
            _G.ProBaconHubGuiLaunched = true
			game.Players.LocalPlayer.PlayerGui.ProBaconHub:Destroy()
		end
	end
	
	game.Players.LocalPlayer.PlayerGui.ProBaconHub.Loading.Launch.MouseButton1Click:Connect(LeftClickOnLaunch)
end
coroutine.wrap(COOMEEN_fake_script)()
local function VEXNAV_fake_script() -- Dismis.LocalScript 
	local script = Instance.new('LocalScript', Dismis)

	local function LeftClickOnDismis()
		game.Players.LocalPlayer.PlayerGui.ProBaconHub:Destroy()
	end
	
	game.Players.LocalPlayer.PlayerGui.ProBaconHub.Loading.Dismis.MouseButton1Click:Connect(LeftClickOnDismis)
end
coroutine.wrap(VEXNAV_fake_script)()
