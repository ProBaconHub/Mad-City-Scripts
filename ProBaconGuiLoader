local function OPWZ_fake_script() -- Progress.DownloadProgress 
	local script = Instance.new('LocalScript', Progress)

	_G.ProBaconHubLoadDataDone = false
    _G.ProBaconHubGuiLunched = false
	local script = Instance.new('LocalScript', Progress)
	
	print("Loading script, please wait.")
	local Head = "Download: "
	game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="Downloading Data..."; Duration=5;})
	task.wait(0.5)
	game.StarterGui:SetCore("SendNotification", {Title="Pro Bacon"; Text="May take a few seconds to load..."; Duration=5;})
	game.Players.LocalPlayer.PlayerGui.ProBaconHub.Loading.Progress.Text = "Downloading Data..."
	task.wait(1)
	game.Players.LocalPlayer.PlayerGui.ProBaconHub.Loading.Progress.Text = Head.."VersionOfFunction"
